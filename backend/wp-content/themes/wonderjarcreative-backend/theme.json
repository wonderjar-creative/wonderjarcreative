{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "palette": [
        {
          "slug": "teal",
          "name": "Teal",
          "color": "#50C9B4"
        },
        {
          "slug": "warm-coral",
          "name": "Warm Coral",
          "color": "#F2685E"
        },
        {
          "slug": "muted-brick-red",
          "name": "Muted Brick Red",
          "color": "#C24D53"
        },
        {
          "slug": "deep-black",
          "name": "Deep Black",
          "color": "#000000"
        },
        {
          "slug": "retro-teal",
          "name": "Retro Teal",
          "color": "#4D7A7F"
        },
        {
          "slug": "soft-cream",
          "name": "Soft Cream",
          "color": "#EFE5D5"
        },
        {
          "slug": "charcoal-gray",
          "name": "Charcoal Gray",
          "color": "#4E5664"
        },
        {
          "slug": "marigold-yellow",
          "name": "Marigold Yellow",
          "color": "#EAA221"
        }
      ]
    },
    "layout": {
      "contentSize": "645px",
      "wideSize": "1280px"
    },
    "spacing": {
      "defaultSpacingSizes": false,
      "spacingSizes": [
        {
          "name": "Tiny",
          "size": "10px",
          "slug": "20"
        },
        {
          "name": "X-Small",
          "size": "20px",
          "slug": "30"
        },
        {
          "name": "Small",
          "size": "30px",
          "slug": "40"
        },
        {
          "name": "Regular",
          "size": "clamp(30px, 5vw, 50px)",
          "slug": "50"
        },
        {
          "name": "Large",
          "size": "clamp(30px, 7vw, 70px)",
          "slug": "60"
        },
        {
          "name": "X-Large",
          "size": "clamp(50px, 7vw, 90px)",
          "slug": "70"
        },
        {
          "name": "XX-Large",
          "size": "clamp(70px, 10vw, 140px)",
          "slug": "80"
        }
      ],
      "units": [
        "%",
        "px",
        "em",
        "rem",
        "vh",
        "vw"
      ]
    },
    "typography": {
      "writingMode": true,
      "defaultFontSizes": false,
      "fluid": true,
      "fontSizes": [
        {
          "fluid": false,
          "name": "Small",
          "size": "0.88889rem",
          "slug": "small"
        },
        {
          "fluid": false,
          "name": "Medium",
          "size": "1.125rem",
          "slug": "medium"
        },
        {
          "fluid": {
            "max": "1.375rem",
            "min": "1.125rem"
          },
          "name": "Large",
          "size": "1.38rem",
          "slug": "large"
        },
        {
          "fluid": {
            "max": "2rem",
            "min": "1.75rem"
          },
          "name": "Extra Large",
          "size": "1.75rem",
          "slug": "x-large"
        },
        {
          "fluid": {
            "max": "3rem",
            "min": "2.15rem"
          },
          "name": "Extra Extra Large",
          "size": "2.15rem",
          "slug": "xx-large"
        }
      ]
    },
    "useRootPaddingAwareAlignments": true
  }
}